{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "properties": {
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The First_name Schema",
      "default": "",
      "examples": [
        "bob"
      ],
      "pattern": "^(.*)$"
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema",
      "default": "",
      "examples": [
        "johnson"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "bob@gmail.com"
      ],
      "pattern": "^(.*)$",
      "format": "email"
    },
    "photo_url": {
      "$id": "#/properties/photo_url",
      "type": "string",
      "title": "The Photo_Url Schema",
      "default": "",
      "examples": [
        "http://www.photourl.com/profile.jpg"
      ],
      "pattern": "^(.*)$",
      "format": "uri"
    }
  }
}